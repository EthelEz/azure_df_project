{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "labadf202401"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"ln_sql_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ln_sql_db'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://lab-akv-accesspolicy01.vault.azure.net/"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "eshicareserver.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "eshicaredb"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "eshicareadmin"
		},
		"ln_asa__properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://eshicare061324.blob.core.windows.net/"
		},
		"ln_sql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "eshicareserver.database.windows.net"
		},
		"ln_sql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "eshicaredb"
		},
		"ln_sql_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "eshicareadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "labadf202401_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"ln_sql_db_password": {
						"value": "[parameters('ln_sql_db_password')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"ln_asa__properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('ln_asa__properties_typeProperties_serviceEndpoint')]"
					},
					"ln_sql_db_properties_typeProperties_server": {
						"value": "[parameters('ln_sql_db_properties_typeProperties_server')]"
					},
					"ln_sql_db_properties_typeProperties_database": {
						"value": "[parameters('ln_sql_db_properties_typeProperties_database')]"
					},
					"ln_sql_db_properties_typeProperties_userName": {
						"value": "[parameters('ln_sql_db_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}